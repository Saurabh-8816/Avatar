public static void IsPalindrome(int num){
          
          int rem = 0;
          int original = num;

          while(num>0){
        int last_digit = num % 10;
        rem = (rem *10) + last_digit;
        num = num /10;
          }
          
        if (rem == original){
            System.out.print("Given number is palindrome!");
          }

        else{
            System.out.print("Given number isn't palindrome!");
            }
    }
